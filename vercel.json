{
  "version": 2,
  "builds": [
    { "src": "frontend/package.json", "use": "@vercel/next" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/frontend/$1" }
  ],
  "env": {
    "API_KEY": "seu_valor_aqui",
    "NEXT_PUBLIC_ANALYTICS_ID": "seu_valor_aqui"
  },
  "functions": {
    "frontend/api/**/*.js": { "memory": 1024, "maxDuration": 10 }
  }
}
